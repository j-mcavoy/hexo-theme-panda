
<div class="gallery-container">
    <% if (item.photos.length){ %>
        <% item.photos.forEach(function(photo){ %>
            <div class="picture-container">
                <a href="#" data-featherlight="<%- config.root %><%- item.path %><%- photo %>">
                    <img src="<%- config.root %><%- item.path %><%- photo %>" class="img-thumbnail" />
                </a>
            </div>
        <% }); %>
    <% } %>
</div>
